    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Aktuelle Datei",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Proxy",
            "type": "python",
            "request": "launch",
            "program": "proxy/proxy.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Trappist-1f",
            "type": "python",
            "request": "launch",
            "program": "server/server.py",
            "args": ["ws://localhost:8080","trappist-1f"],
            "console": "integratedTerminal"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}